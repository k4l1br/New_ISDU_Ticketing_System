[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Http\Controllers\Admin;[m
[32m+[m
[32m+[m[32muse App\Http\Controllers\Controller;[m
[32m+[m[32muse App\Models\User;[m
[32m+[m[32muse Illuminate\Http\Request;[m
[32m+[m[32muse Illuminate\Support\Facades\Hash;[m
[32m+[m[32muse Illuminate\Validation\Rules;[m
[32m+[m
[32m+[m[32mclass UserController extends Controller[m
[32m+[m[32m{[m
[32m+[m[41m    [m
[32m+[m[32m    public function index(Request $request)[m
[32m+[m[32m{[m
[32m+[m[32m     $users = User::query()[m
[32m+[m[32m            ->when($request->name, fn($q, $name) => $q->where('name', 'like', "%$name%"))[m
[32m+[m[32m            ->when($request->email, fn($q, $email) => $q->where('email', 'like', "%$email%"))[m
[32m+[m[32m            ->when($request->role, fn($q, $role) => $q->where('role', $role))[m
[32m+[m[32m            ->orderBy('name')[m
[32m+[m[32m            ->paginate(10);[m
[32m+[m
[32m+[m[32m        return view('admin.users.index', compact('users'));[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m    public function create()[m
[32m+[m[32m    {[m
[32m+[m[32m        return view('admin.users.create');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function store(Request $request)[m
[32m+[m[32m    {[m
[32m+[m[32m    $request->validate([[m
[32m+[m[32m        'name' => 'required|string|max:255',[m
[32m+[m[32m        'email' => 'required|string|email|max:255|unique:users',[m
[32m+[m[32m        'role' => 'required|string',[m
[32m+[m[32m        'password' => 'required|string|min:8|confirmed',[m
[32m+[m[32m    ]);[m
[32m+[m
[32m+[m[32m    User::create([[m
[32m+[m[32m        'name' => $request->name,[m
[32m+[m[32m        'email' => $request->email,[m
[32m+[m[32m        'role' => $request->role,[m
[32m+[m[32m        'password' => Hash::make($request->password),[m
[32m+[m[32m    ]);[m
[32m+[m
[32m+[m[32m    return redirect()->route('admin.users.index')[m
[32m+[m[32m           ->with('success', 'User created successfully.');[m
[32m+[m[32m    }[m
[32m+[m[32m    public function show(User $user)[m
[32m+[m[32m    {[m
[32m+[m[32m        return view('admin.users.show', compact('user'));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function edit(User $user)[m
[32m+[m[32m    {[m
[32m+[m[32m        return view('admin.users.edit', compact('user'));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function update(Request $request, User $user)[m
[32m+[m[32m    {[m
[32m+[m[32m        $request->validate([[m
[32m+[m[32m            'name' => ['required', 'string', 'max:255'],[m
[32m+[m[32m            'email' => ['required', 'string', 'email', 'max:255', 'unique:users,email,'.$user->id],[m
[32m+[m[32m            'role' => ['required', 'string'],[m
[32m+[m[32m        ]);[m
[32m+[m
[32m+[m[32m        $user->update($request->only('name', 'email', 'role'));[m
[32m+[m
[32m+[m[32m        if ($request->filled('password')) {[m
[32m+[m[32m            $user->update([[m
[32m+[m[32m                'password' => Hash::make($request->password),[m
[32m+[m[32m            ]);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return redirect()->route('admin.users.index')[m
[32m+[m[32m               ->with('success', 'User updated successfully.');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function destroy(User $user)[m
[32m+[m[32m    {[m
[32m+[m[32m        $user->delete();[m
[32m+[m[32m        return redirect()->route('admin.users.index')[m
[32m+[m[32m               ->with('success', 'User deleted successfully.');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[41m    [m
[32m+[m[32m}[m
