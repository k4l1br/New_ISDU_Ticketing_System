[36m@@ -0,0 +1,48 @@[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Http\Controllers;[m
[32m+[m
[32m+[m[32muse Illuminate\Http\Request;[m
[32m+[m[32muse Illuminate\Support\Facades\Auth;[m
[32m+[m[32muse Illuminate\Support\Facades\Hash;[m
[32m+[m[32muse Illuminate\Validation\Rules\Password;[m
[32m+[m
[32m+[m[32mclass PasswordController extends Controller[m
[32m+[m[32m{[m
[32m+[m[32m    // Show password change form[m
[32m+[m[32m    public function edit()[m
[32m+[m[32m    {[m
[32m+[m[32m        return view('passwords.edit');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Update password[m
[32m+[m[32m    public function update(Request $request)[m
[32m+[m[32m    {[m
[32m+[m[32m        $request->validate([[m
[32m+[m[32m            'current_password' => ['required', function ($attribute, $value, $fail) {[m
[32m+[m[32m                /** @var \App\Models\User $user */[m
[32m+[m[32m                $user = Auth::user();[m
[32m+[m[32m                if (!Hash::check($value, $user->password)) {[m
[32m+[m[32m                    $fail('The current password is incorrect.');[m
[32m+[m[32m                }[m
[32m+[m[32m            }],[m
[32m+[m[32m            'new_password' => [[m
[32m+[m[32m                'required',[m
[32m+[m[32m                'confirmed',[m
[32m+[m[32m                Password::min(8)[m
[32m+[m[32m                    ->mixedCase()[m
[32m+[m[32m                    ->numbers()[m
[32m+[m[32m                    ->symbols()[m
[32m+[m[32m                    ->uncompromised(),[m
[32m+[m[32m            ],[m
[32m+[m[32m        ]);[m
[32m+[m
[32m+[m[32m        /** @var \App\Models\User $user */[m
[32m+[m[32m        $user = Auth::user();[m
[32m+[m[32m        $user->update([[m
[32m+[m[32m            'password' => Hash::make($request->new_password)[m
[32m+[m[32m        ]);[m
[32m+[m
[32m+[m[32m        return back()->with('success', 'Password changed successfully!');[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
